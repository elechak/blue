x=0;
sys.print(x, "\n");
x==0  ? sys.print("T") : sys.print("F");
x==1  ? sys.print("T") : sys.print("F");
x==-1 ? sys.print("T") : sys.print("F");
sys.print("\n");
x!=0  ? sys.print("T") : sys.print("F");
x!=1  ? sys.print("T") : sys.print("F");
x!=-1 ? sys.print("T") : sys.print("F");
sys.print("\n");
x < 1 ? sys.print("T") : sys.print("F");
x < 0 ? sys.print("T") : sys.print("F");
x < -1 ? sys.print("T") : sys.print("F");
sys.print("\n");
x <= 1 ? sys.print("T") : sys.print("F");
x <= 0 ? sys.print("T") : sys.print("F");
x <= -1 ? sys.print("T") : sys.print("F");
sys.print("\n");
x > 1 ? sys.print("T") : sys.print("F");
x > 0 ? sys.print("T") : sys.print("F");
x > -1 ? sys.print("T") : sys.print("F");
sys.print("\n");
x >= 1 ? sys.print("T") : sys.print("F");
x >= 0 ? sys.print("T") : sys.print("F");
x >= -1 ? sys.print("T") : sys.print("F");
sys.print("\n");
!(x==0)  ? sys.print("T") : sys.print("F");
!(x==1)  ? sys.print("T") : sys.print("F");
!(x==-1) ? sys.print("T") : sys.print("F");
sys.print("\n");
!(x!=0)  ? sys.print("T") : sys.print("F");
!(x!=1)  ? sys.print("T") : sys.print("F");
!(x!=-1) ? sys.print("T") : sys.print("F");
sys.print("\n");
!(x < 1) ? sys.print("T") : sys.print("F");
!(x < 0) ? sys.print("T") : sys.print("F");
!(x < -1) ? sys.print("T") : sys.print("F");
sys.print("\n");
!(x <= 1) ? sys.print("T") : sys.print("F");
!(x <= 0) ? sys.print("T") : sys.print("F");
!(x <= -1) ? sys.print("T") : sys.print("F");
sys.print("\n");
!(x > 1) ? sys.print("T") : sys.print("F");
!(x > 0) ? sys.print("T") : sys.print("F");
!(x > -1) ? sys.print("T") : sys.print("F");
sys.print("\n");
!(x >= 1) ? sys.print("T") : sys.print("F");
!(x >= 0) ? sys.print("T") : sys.print("F");
!(x >= -1) ? sys.print("T") : sys.print("F");
